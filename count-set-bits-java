import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	     Scanner sc = new Scanner(System.in);
	     System.out.println("Enter the number of test cases : ");
	     int t = sc.nextInt();
	     while(t-->0){
	         System.out.println("Enter the number :");
	         int n = sc.nextInt();
	         int res = 0;
	         while(n>0){
	             n = n&(n-1);
	             res++;
	         }
	         System.out.println("the number of set bits are :" +res);
	         
	     }
	 }
 }
